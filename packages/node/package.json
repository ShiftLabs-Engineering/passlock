{
  "name": "@passlock/node",
  "type": "module",
  "version": "0.9.23",
  "description": "Passkey authentication and social login for node.js/express apps",
  "keywords": [
    "passkey",
    "passkeys",
    "webauthn",
    "node",
    "express"
  ],
  "author": {
    "name": "Toby Hobson",
    "email": "toby@passlock.dev"
  },
  "license": "MIT",
  "homepage": "https://passlock.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/passlock-dev/ts-clients.git",
    "directory": "packages/node"
  },
  "bugs": {
    "url": "https://github.com/passlock-dev/ts-clients/issues",
    "email": "team@passlock.dev"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "src",
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "typecheck": "tsc --project tsconfig.build.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest dev",
    "test:ui": "vitest --coverage.enabled=true --ui",
    "test:coverage": "vitest run --coverage",
    "build": "tsc --project tsconfig.build.json",
    "build:clean": "pnpm run clean && pnpm run build",
    "build:readme": "LATEST=${npm_package_version} tsx scripts/replace-readme-tokens.ts",
    "replaceTokens": "LATEST=${npm_package_version} tsx scripts/replace-tokens.ts",
    "build:production": "pnpm run replaceTokens && echo '' && publint",
    "format": "prettier --write \"src/**/*.+(js|ts|json)\"",
    "lint": "eslint --ext .ts src",
    "lint:fix": "pnpm run lint --fix",
    "prepublishOnly": "pnpm run build:production",
    "upgrade:deps": "ncu --peer -x effect -x @effect/* -u"
  },
  "dependencies": {
    "@passlock/shared": "workspace:*",
    "effect": "3.6.3"
  },
  "devDependencies": {
    "@effect/schema": "0.70.4",
    "@qetza/replacetokens": "^1.7.0",
    "@total-typescript/tsconfig": "^1.0.4",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.2.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "publint": "^0.2.9",
    "rimraf": "^6.0.1",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vitest": "^2.0.5"
  }
}
