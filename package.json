{
  "name": "public",
  "description": "Monorepo containing the JS/TS client libraries",
  "version": "0.9.23",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "pnpm -r run clean",
    "build": "pnpm -r run build",
    "build:clean": "pnpm -r run build:clean",
    "build:readme": "LATEST=${npm_package_version} tsx scripts/replace-readme-tokens.ts && pnpm -r run build:readme",
    "build:production": "LATEST=${npm_package_version} tsx scripts/replace-readme-tokens.ts && pnpm -r run build:production",
    "format": "pnpm -r run format",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint:fix",
    "upgrade:deps": "ncu --peer -x effect -x @effect/* -x @passlock/* -u && pnpm -r run upgrade:deps",
    "upgrade:pnpm": "corepack use pnpm@latest"
  },
  "devDependencies": {
    "@passlock/shared": "workspace:^",
    "@total-typescript/tsconfig": "^1.0.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.2.0",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  },
  "pnpm": {
    "overrides": {
      "effect": "3.6.3",
      "@effect/schema": "0.70.4",
      "@passlock/sveltekit": "workspace:*",
      "@passlock/client": "workspace:*",
      "@passlock/shared": "workspace:*",
      "svelte-preprocess": "^6.0.2",
      "@aws-sdk/lib-dynamodb": "3.395.0",
      "@aws-sdk/client-dynamodb": "3.395.0",
      "@aws-sdk/client-sts": "3.606.0",
      "@aws-sdk/client-sso-oidc": "3.606.0"
    }
  },
  "packageManager": "pnpm@9.7.0+sha256.b35018fbfa8f583668b2649e407922a721355cd81f61beeb4ac1d4258e585559"
}
